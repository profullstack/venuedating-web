{
  "name": "barcrush-web",
  "version": "0.0.1",
  "description": "BarCrush web application with document generation and venue matching",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "playwright test",
    "start": "node index.js",
    "dev:hono": "pnpm build && nodemon index.js",
    "deploy": "pnpm build && ./bin/deploy.sh",
    "build:legacy": "./bin/build.sh",
    "stripe:products": "node bin/create-stripe-products.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "chalk": "^5.3.0",
    "charts.css": "^1.1.0",
    "docx": "^9.5.0",
    "dotenv": "^16.5.0",
    "dotenv-flow": "^4.1.0",
    "form-data": "^4.0.2",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.542.0",
    "mailgun.js": "^12.0.1",
    "marked": "^15.0.8",
    "puppeteer": "^22.5.0",
    "qrcode": "^1.5.4",
    "square": "^39.0.0",
    "stripe": "^14.22.0",
    "twilio": "^5.7.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.1",
    "xlsx": "^0.18.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  }
}
